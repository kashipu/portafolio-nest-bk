{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "uses": ["@vercel/node"]
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}